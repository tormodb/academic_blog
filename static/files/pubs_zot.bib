@article{HeradstveitProspectiveassociationschildhood2018,
  ids = {heradstveit2018prospective},
  title = {Prospective Associations between Childhood Externalising and Internalising Problems and Adolescent Alcohol and Drug Use: {{The Bergen Child Study}}},
  shorttitle = {Prospective Associations between Childhood Externalising and Internalising Problems and Adolescent Alcohol and Drug Use},
  author = {Heradstveit, Ove and Skogen, Jens Christoffer and BÃ¸e, Tormod and Hetland, JÃ¸rn and Pedersen, Mads Uffe and Hysing, Mari},
