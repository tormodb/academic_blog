@article{BoeFinancialdifficultieschildhood2017,
  ids = {7,boe2017financial},
  title = {Financial Difficulties in Childhood and Adult Depression in {{Europe}}},
  author = {BÃ¸e, Tormod and Balaj, Mirza and Eikemo, Terje A. and Mcnamara, Courtney L. and Solheim, Erling F.},
  date = {2017},
  journaltitle = {Eur. J. Public Health},
  volume = {27},
  number = {1},
  pages = {96--101},
  publisher = {{Oxford University Press}},
  issn = {1101-1262},
  doi = {10/f9vrfd},
  abstract = {Background: financial difficulties in childhood may be associated with immediate and long-term consequences for mental health. the aim of the current paper is to investigate the association between childhood financial difficulties and adult depression, and assess the relative contribution of financial difficulties in childhood to symptoms of adult depression across different age groups. methods: using three age cohorts (25â€“40, 41â€“59, 60â€“75) from 19 countries in the european social survey round 7 (n =18 401), multi-level and country-wise ols regression analyses were used to investigate the association between financial difficulties in childhood and adult depression, while adjusting for age, education, gender, highest education in family, level of family conflict, number of social meetings and marital status. results: financial difficulties in childhood was found to be influ-ential predictors of depression scores for 25â€“40 year olds in 10 out of 19 countries in fully adjusted models. in older participants, depression scores were mostly influenced by frequency of social meetings and marital status. there was great variation in the pattern of influential risk factors across countries, and the predicted effect childhood financial difficulties had on adult depression scores. conclusion: childhood financial difficulties as predictors of depression appear to, by themselves, exert the strongest influence in younger adults. there was, however, large variation between countries in the magnitude of associated risk, and in the pattern of risk factors contributing to adult depression, which underscores the need to account for country-level factors when aiming to gain knowledge about mental health.},
  file = {/Users/st06810/Dropbox/References/bib/BÃ¸e et al_2017_Financial difficulties in childhood and adult depression in Europe.pdf;/Users/st06810/Zotero/storage/7S98KBCU/3045956.html;/Users/st06810/Zotero/storage/J5LQU74Q/3045956.html;/Users/st06810/Zotero/storage/K663UNQ4/3045956.html;/Users/st06810/Zotero/storage/L9IU2I4T/3045956.html;/Users/st06810/Zotero/storage/LUM26Z64/3045956.html;/Users/st06810/Zotero/storage/QSVKNAV9/3045956.html}
}

@article{BoeFinancialDifficultiesStudent2020,
  ids = {BoeFinancialDifficultiesStudent2021},
  title = {Financial {{Difficulties}} and {{Student Health}}: {{Results}} from a {{National Cross-Sectional Survey}} of {{Norwegian College}} and {{University}} Students},
  shorttitle = {Financial {{Difficulties}} and {{Student Health}}},
  author = {BÃ¸e, Tormod and Hysing, Mari and LÃ¸nning, Kari Jussie and Sivertsen, BÃ¸rge},
  date = {2020},
  journaltitle = {Mental Health \& Prevention},
  pages = {200196},
  issn = {2212-6570},
  doi = {10/ghrkfn},
  abstract = {To examine financial circumstances among Norwegian higher education students and investigate associations between financial difficulties and health, academic outcomes and self-destructive behaviours, we used data from a recent national health survey from 2018 for higher education in Norway (the SHoT study). 50,054 full-time students (69.1\% women; 30.9\% men) aged 18â€“35 years participated and reported work status, income and experience of financial difficulties, self-reported mental health problems, deliberate self-harm and suicide attempts, somatic health complaints and exam failures in higher education. Students often experiencing financial difficulties (n\,=\,3933, 7.9\%) reported more mental health problems (HSCL-25; M\,=\,2.1 vs 1.6), depression (24\% vs 7.2\%), anxiety (20.8\% vs 6.7\%), deliberate self-harm (12.2\% vs 4.9\%), attempted suicide (3.1\% vs 0.5\%), somatic health complaints (SSS8; M\,=\,13.1 vs 7.4), having ever failed exams (45.5\% vs 28.4\%) and higher average number of failed exams (M\,=\,1.1 vs 0.6) compared to peers not experiencing financial difficulties. These associations were robust to adjustments for age, gender, relationship status, immigrant background and study program. Strategies to alleviate problems associated with financial difficulties may be to improve the existing financial support to students, strengthen student mental health services, and expand services for financial counselling and guidance.},
  langid = {english},
  file = {/Users/st06810/Dropbox/References/bib/BÃ¸e et al_2020_Financial Difficulties and Student Health.pdf;/Users/st06810/Zotero/storage/C7Z48KIX/S2212657020302269.html;/Users/st06810/Zotero/storage/L3M68TX6/S2212657020302269.html}
