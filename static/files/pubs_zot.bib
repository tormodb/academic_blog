@article{NilsenMentalhealthadverse2021,
  title = {Mental Health, Adverse Life Events and Health Service Use among {{Norwegian}} Youth in the Child Welfare System: {{Results}} from a Population-Based Study},
  shorttitle = {Mental Health, Adverse Life Events and Health Service Use among {{Norwegian}} Youth in the Child Welfare System},
