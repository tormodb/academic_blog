@article{BoeParentalEducationRelatedInequality2021,
  title = {Do {{Parental Education-Related Inequality Matter}} in {{Child}} and {{Adolescent Utilization}} of {{Mental Health Services}}: {{Results From}} a {{Norwegian Register Linkage Study}}},
  shorttitle = {Do {{Parental Education-Related Inequality Matter}} in {{Child}} and {{Adolescent Utilization}} of {{Mental Health Services}}},
